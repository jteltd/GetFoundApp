    "Privacy-Cookie-Policy.html": {
      "settings": {
        "title": "Privacy Policy",
        "order": 2,
        "meta_descr": "Privacy and Cookie Policy",
        "header_custom": ""
      },
      "components": [
        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
